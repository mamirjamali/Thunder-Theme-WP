{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
  "apiVersion": 2,
  "name": "thunder-plus/team-members-group",
  "title": "Team Members Group",
  "category": "text",
  "description": "Adds a group for adding team members",
  "textdomain": "thunder-plus",
  "attributes": {
    "columns": {
      "type": "number",
      "default": 3
    },
    "imageShape": {
      "type": "string",
      "default": "hexagon"
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./index.css",
  "providesContext": {
    "thunder-plus/image-shape": "imageShape"
  },
  "example": {
    "attributes": {
      "columns": 2
    },
    "innerBlocks": [
      {
        "name": "thunder-plus/team-member",
        "attributes":{
          "Name": "John smith",
          "title": "CEO of Google",
          "bio": "This a examole bio",
          "imgURL": "https://i.picsum.photos/id/27/3264/1836.jpg?hmac=p3BVIgKKQpHhfGRRCbsi2MCAzw8mWBCayBsKxxtWO8g"
        }
      },
      {
        "name": "thunder-plus/team-member",
        "attributes":{
          "Name": "John Doe",
          "title": "CEO of Google",
          "bio": "This a examole bio",
          "imgURL": "https://i.picsum.photos/id/27/3264/1836.jpg?hmac=p3BVIgKKQpHhfGRRCbsi2MCAzw8mWBCayBsKxxtWO8g"
        }
      }
    ]
  }
}